<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="48.0" prefWidth="416.0" styleClass="todo_pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <children>
      <AnchorPane prefHeight="48.0" prefWidth="48.0" styleClass="todo_date">
         <children>
            <Text fx:id="day" layoutX="17.0" layoutY="21.0" strokeType="OUTSIDE" strokeWidth="0.0" text="11" textAlignment="CENTER" />
            <Text fx:id="month" layoutX="12.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nov" textAlignment="CENTER" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="58.0" prefHeight="48.0" prefWidth="358.0" style="-fx-background-color: White;" styleClass="base">
         <children>
            <Text fx:id="name" layoutX="10.0" layoutY="22.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="todo_title" text="Finish 6200 Final Project UI Design" />
            <Text fx:id="remain" layoutX="10.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="todo_status" text="Overdue">
               <font>
                  <Font size="10.0" />
               </font>
            </Text>
            <ImageView fx:id="t_edit" fitHeight="15.0" fitWidth="20.0" layoutX="280.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../assets/icons/edit.png" />
               </image></ImageView>
            <ImageView fx:id="t_remove" fitHeight="15.0" fitWidth="20.0" layoutX="300.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../assets/icons/delete.png" />
               </image></ImageView>
            <ImageView fx:id="t_done" fitHeight="15.0" fitWidth="20.0" layoutX="320.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../assets/icons/check%20box.png" />
               </image></ImageView>
         </children>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@../../resources/Styles/todopage.css" />
      <URL value="@../../resources/Styles/main.css" />
   </stylesheets>
</AnchorPane>
